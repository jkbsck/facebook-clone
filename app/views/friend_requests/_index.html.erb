<% current_user.friend_requests_as_receiver.each do |request| %>
  <% unless request.accepted == true %>
    Request from <b><%= request.requestor.username %></b>
    to <b><%= request.receiver.username %></b><br>
  <% end %>
<% end %><br>

<% @friend_requests.each do |r| %>
  <%= r.requestor.username + " --> " + r.receiver.username %><br>
<% end %>
